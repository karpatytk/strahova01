{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/admin/(.*)", "destination": "/admin/$1" },
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/js/(.*)", "destination": "/js/$1" },
    { "source": "/public/(.*)", "destination": "/public/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
