{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/js/$1"
    },
    {
      "source": "/style.css",
      "destination": "/style.css"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}